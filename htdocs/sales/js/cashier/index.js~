							  													 start_baht=parseFloat(start_baht);
							  													 var end_baht=end_baht.replace(/[^\d\.\-\ ]/g,'');
							  													 end_baht=parseFloat(end_baht);
							  													 //alert(net_amt +" > " +end_baht);
							  													 if(net_amt>end_baht){
							  														 jAlert("ยอดเงินสุทธิต้องไม่เกิน " + end_baht + " บาท กรุณาทำรายการใหม่", 'แจ้งเตือน',function(){																						
							  															$('#pdt_detail_id').val('').focus();
							  															return false;
							  														});
							  														 return false;																		    										 
							  													 }
							  												//}
							  											}//end if
							  											//*WR28012015 ===========================
							  										}
						  								           
						  											,close:function(evt){
