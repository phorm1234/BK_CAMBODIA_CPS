						if(parseInt(arr_data[0])>0){		
							jAlert('ท่านกำลังทำรายการขาย ไม่สามารถใช้ SMS Pro ได้','ข้อความแจ้งเตือน',function(){
								 return false;
							  });
						}else{
							///////////////
							$('#dlgSmsPro').dialog('destroy');
							$('#dlgSmsPro').dialog(opts_dlgSmsPro);			
							$('#dlgSmsPro').dialog('open');
							//////////////
					    	
						}
					}
				};
			$.ajax(opts_ctemp);
	    	
	    });
	    
