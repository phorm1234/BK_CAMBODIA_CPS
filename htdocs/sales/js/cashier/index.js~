															    			  return false;
															    		  }
															    		  
															    	  });
															      }, 
															      close: function(){
															        $(this).remove();
															      }   
														   });
																$('#doc_no_temp').val('');
																$('#dlgDocNoTemp').dialog('close');															
																return false;
															}
															
															
															var opts_chkshort=null;
															var opts_chk={
																	type:'post',
																	//url:'/sales/cashier/chkfreeshort',
																	url:'/sales/returns/chkfreeshort',
