							  else
							    $(this).css("background-color", "#ccffff");
						});
						*/						
					},
					onError:function(data){
						alert("ERROR : "+data);
					},
					showTableToggleBtn:true,
					striped:false,
					height:gHeight
				}
		);//end flexigrid	
		
