					   			        if(key == 13){	
						   			        var product_id=$.trim($("#adt_product_id").val());
						   			        if(product_id.length==0){
						   			        	jAlert('กรุณาระบุรหัสสินค้า', 'กรุณาตรวจสอบอีกครั้ง',function(){		
						   			        		$("#adt_product_id").focus();																				
