				        grid.find('tr').each(function() {
					        c_promo_code=$("td div", $(this)).eq(2).text();	
					        if(c_promo_code!=r_promo_code){
					        	r_promo_code=c_promo_code;
					        	if(r_bgcolor!="#FFFFFF"){
					        		r_bgcolor="#FFFFFF";
					        	}else{
					        		r_bgcolor="#ffcc99";
					        	}
					        }
					        $(this).css("background-color",r_bgcolor);
				        }); 
						/*
