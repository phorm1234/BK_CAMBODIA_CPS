								   			    $("#res_qproduct").html('');
								   			    $.ajax({
									   			    	type:'post',
									   			    	url:'/sales/cashier/qestpdtofbill',
									   			    	data:{
								   			    			product_id:product_id,
								   			    			rmd:Math.random
								   			    		},
								   			    		success:function(data){
								   			    			$("#res_qproduct").html(data);
									   			    	}
									   			    	
									   			    });
								   			}
						   			        return false;
					   			        }
						   			 });
							},close:function(){
				   				$(this).remove();
				   			}
				});
			//stop
		});

