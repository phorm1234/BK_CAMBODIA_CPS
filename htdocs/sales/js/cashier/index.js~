					$('#ajaxBusy').hide();
				}
				var dlgUpdData = $('<div id="dlgUpdData"><div id="myProgressBar"><span id="pg-status" class="pg-status"></span></div></div>');
	            if($("#dlgUpdData").dialog( "isOpen" )===true) {
	            	return false;
	            }//if
	            var p=0;
	            dlgUpdData.dialog({
		           autoOpen:true,
				   width:'500',
				   height:'100',	
				   modal:true,
				   resizable:false,
		           title: "กรุณารอสักครู่... ",		
		           position:"center",
		            open:function(){		
		            	$(this).dialog('widget')
			            .find('.ui-dialog-titlebar')
			            .removeClass('ui-corner-all')
			            .addClass('ui-corner-top');
		            	//run process background
					    
		            	var objReqUp2Data=$.ajax({
							type:'post',
							url:'/sales/accessory/reupddata',
							cache:false,
							data:{
								rnd:Math.random()
							},
							success:function(){
								objReqUp2Data=null;
