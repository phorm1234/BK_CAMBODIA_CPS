	function lineApp3(application_id,a,b,c,d,e,idcard,coupon_code,friend_id_card,friend_mobile_no){
		 /***
		  * @desc
		  * @create WR 05052015  OPMGMC300
		  */
		 var str_coupon=friend_id_card+"#"+friend_mobile_no;
		   if(application_id=='OPMGMC300'){
			   $("#csh_mobile_no").val(coupon_code);
		   }
		    $("#csh_id_card").val(idcard);
			$("#csh_coupon_code").val(str_coupon);
			getCatProduct(application_id,0,0,0,0,0);
			return false;
	 }//func
		 
	 function lineApp2(application_id,a,b,c,d,e,idcard,coupon_code){
		 /**
		  * @desc
		  * @create WR 20122015
		  */
		 var str_coupon=idcard+"#"+coupon_code;
			//alert("str_coupon=>" +str_coupon);
			$("#csh_coupon_code").val(str_coupon);
			getCatProduct(application_id,0,0,0,0,0);
			return false;
	 }//func
	 
	 function lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code){
			/**
			 * @desc
			 * @create 20012015
			 * @return
			 */
		   //alert(promo_code +","+chk_status+","+line_msg_error +","+ idcard +","+ coupon_code);
			if(chk_status=='Y'){
				//*WR 08052015
				$('#status_couponpromo').val('N');
				if(promo_code=='OX02230315'){
					var str_coupon=promo_code+"#"+coupon_code;
					$("#csh_coupon_code").val(str_coupon);
				}
				
				//*WR13022015
				if(promo_code=='OI27010115'){
					var str_coupon=idcard+"#"+coupon_code;
					$("#csh_coupon_code").val(str_coupon);
				}
				
				//*WR23012015
				if(promo_code=='OI27120115'){
					$('#status_couponpromo').val('Y');
					$('#csh_coupon_code').val(promo_code);
					$('#csh_id_card').val(idcard);
				}
				
				//*WR30012015
				if(promo_code=='OI27150115'){
					//on top discount 100
					$('#status_couponpromo').val('Y');
					$('#csh_coupon_code').val(coupon_code);
					$('#csh_id_card').val(idcard);
				}
				
				if($("#dlgPdtMCoupon").dialog( "isOpen" )===true) {
	            	return false;
	            }//if
				
				$('<div id="dlgPdtMCoupon"><span style="color:#336666;font-size:20px;">รหัสสินค้า : </span><input type="text" size="15" id="pdt_mcoupon"/></div>').dialog({
			           autoOpen:true,
					   width:'400',
					   height:'200',	
					   modal:true,
					   resizable:false,
					   closeOnEscape:false,		
			           title: "สแกนรายการสินค้า",
			           position:"center",
			           open:function(){
							$(this).dialog('widget')
					            .find('.ui-dialog-titlebar')
					            .removeClass('ui-corner-all')
					            .addClass('ui-corner-top');		
							//button style	
			  			    $(this).dialog("widget").find(".ui-btndlgpos")
			                  .css({"padding":"2","background-color":"#ffd1dc","font-size":"14px"});
			  			  $(this).dialog("widget").find(".ui-widget-overlay").live('click', function(){
						    	$("#pdt_redeempoint").focus();
							});
				  		  $('#pdt_mcoupon').keypress(function(evt){																    	
							    	var key = evt.keyCode ? evt.keyCode : evt.which ? evt.which : evt.charCode; 
				   			        if(key == 13){	
						    			evt.preventDefault();
						    			evt.stopPropagation();
						    			$('#btnMCoupon').trigger('click');
						    			return false;
				   			        }
							    });
			           },
	    				buttons: [ 
					    	            { 
					    	                text: "ตกลง",
					    	                id:"btnMCoupon",
					    	                class: 'ui-btndlgpos', 
					    	                click: function(evt){ 
					    	                	evt.preventDefault();
										 		evt.stopPropagation();
										 		var product_id=$('#pdt_mcoupon').val();
										 		product_id=$.trim(product_id);																		 			
									    		if(product_id.length<1){
								    				jAlert('กรุณาสแกนรหัสสินค้า', 'แจ้งเตือน',function(){
								    					$('#pdt_mcoupon').val('').focus();
								    					return false;
								    				});	
								    				return false;
									    		}
									    		//alert(m_promo_code + " ==> " + promo_id);
									    		var member_no=$('#csh_member_no').val();																													    	
									    		var status_no=$('#csh_status_no').val();
									    		//*WR12012015
									    		var member_percent=$('#csh_percent_discount').html();
					  							 $.ajax({
					  								 type:'post',
					  								 url:'/sales/member/setpdtmcoupon',
					  								 data:{
					  									 member_no:member_no,																									  									
					  									 promo_code:promo_code,
					  									 product_id:product_id,
					  									 quantity:'1',
					  									 status_no:status_no,
					  									 member_percent:member_percent,
					  									 rnd:Math.random()
					  								 },success:function(data){			
					  									// alert(data);
					  									var arr_data=data.split('#');
					  									if(arr_data[0]=='1'){				
					  										
					  										//*WR13012015
					  										getCshTemp('P');
					  										$('#pdt_mcoupon').val('').focus();					  										
									    				}else{													    					
									    					jAlert(arr_data[1],'ข้อความแจ้งเตือน',function(){
									    						$('#pdt_mcoupon').val('').focus();
																return false;
															});
									    					
									    				}//end if									  									
					  									
					  								 }
					  							 });//ajax													 		
									 		
				    	                }
				    	            }
				    	  ]			
						,beforeClose:function(){
							//*WR14012015 =======================
							var start_baht=$('#csh_start_baht').val();
							var end_baht=$('#csh_end_baht').val();
							var buy_type=$("#csh_buy_type").val();//Gross,Net
							var buy_status=$("#csh_buy_status").val();//G >=,L<
							if(buy_type=='N' ){
								var net_amt=$('#csh_net').val().replace(/[^\d\.\-\ ]/g,'');
						 		net_amt=parseFloat(net_amt);
								var start_baht=start_baht.replace(/[^\d\.\-\ ]/g,'');
								 start_baht=parseFloat(start_baht);
								 //alert(net_amt +" < " +start_baht);
								 if(net_amt<start_baht){
									 jAlert("ยอดเงินสุทธิอย่างน้อยต้อง " + start_baht + " บาท", 'แจ้งเตือน',function(){																						
										 $('#pdt_mcoupon').val('').focus();
											return false;
									});
									 return false;																		    										 
								 }
								
							}
						
						//*WR14012015 ===========================
						}
						,close:function(evt){
							if(evt.originalEvent && $(evt.originalEvent.target).closest(".ui-dialog-titlebar-close").length){	
								evt.stopPropagation();
	    						evt.preventDefault();					
	    						var net_amt=$('#csh_net').val();					
	    						net_amt=net_amt.replace(/[^\d\.\-\ ]/g,'');	
	    						if(net_amt<1){
	    							jAlert('ไม่พบการทำรายการขายไม่สามารถชำระเงินได้', 'แจ้งเตือน',function(){
	    								initFormCashier();
		    							initFieldOpenBill();
		    							$("#csh_member_no").focus();
				    					return false;
				    				});		    							
	    							return false;
	    						}
							}else if(evt.originalEvent && $(evt.originalEvent.which==27)){
								evt.stopPropagation();
	    						evt.preventDefault();  						
							}
							setTimeout(function(){
								paymentBill('00');
							},400);
							$('#dlgPdtMCoupon').remove();	
			           }
				});
			}else{
				jAlert(line_msg_error, 'แจ้งเตือน',function(){
					initFormCashier();
					initFieldOpenBill();
					$("#csh_member_no").focus();
					return false;
				});
			}
			
			return false;
		}//func lineApp
		///////////////////////////////////////////////////////////xx///////////////////////////////////////////////////////////////////////
	 function callBackToDo(){
			/***
			 * for support callback function
			 * create : 26092014
			 * modify : 27012015 for support mobile coupon and line
			 */		
			var status_no=$('#csh_status_no').val();
			var promo_code=$('#csh_promo_code').val();			
			if(promo_code=='OM02071113' || promo_code=='OM02081113' || promo_code=='OM02091113'){
				$('#csh_product_id').focus();				
			}		
	 }//func
	 
	 function callBackToDo2(promo_code,chk_status,line_msg_error,idcard,coupon_code){
		/***
		 * for support callback function
		 * create : 26092014
		 * modify : 27012015 for support mobile coupon and line
		 */		
		if(chk_status=='Y'){
			$('#status_couponpromo').val('Y');
			$('#csh_coupon_code').val(coupon_code);
			$('#csh_id_card').val(idcard);
			
			//*WR10022015
			if(promo_code=='OK16050215'){
				//อั่งเปา discount 100 buy 500 net 16-28/02/2015
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}
			
			//*WR10022015
			if(promo_code=='OI03180215'){
				//LINE04 แลกซื้อ Princess Garden Talc ที่ร่วมรายการ เหลือ 50 บาท 20-28/02/2015
				setPdt2Temp(promo_code);
				return false;
			}
			
			//*WR10022015
			if(promo_code=='OK04061215'){
				//ซื้อ 1 แถม 1 23/02-01/03/2015
				setPdt2Temp(promo_code);
				 return false;
			}
			//*WR16032015
			if(promo_code=='OK04070315'){
				//ซื้อ 1 แถม 1
				setPdt2Temp(promo_code);
				 return false;
			}
			if(promo_code=='OI27010115'){
				//Line Promotion รับส่วนลด 50% เพื่อเซื้อสินค้าได้ไม่เกิน 3 ชิ้นเท่านั้น
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}
			
			if(promo_code=='OK01160115'){
				//ลด 50% ชิ้นที่ 2 เลือกซื้อสินค้าชนิดใดก็ได้
				setPdt2Temp(promo_code);
				 //buyOneGetOne(promo_code);
				 return false;
			}
			
			if(promo_code=='OI27150115'){
				//coupon discount 100 buy 500 net
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}
			
			//*WR 05052015 member get member
			if(promo_code=='OX02230315'){
				//member get member Promotion รับส่วนลด 50% เพื่อเซื้อสินค้าได้ไม่เกิน 2 ชิ้นเท่านั้น
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}			
			
			
			if(promo_code=='OK27090115'){
				//coupon discount 50 buy 300 net
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}
			
			if(promo_code=='OK09461214'){
				//E-coupon Mobile App. รับส่วนลดพิเศษ 50 บาท เมื่อซื้อสินค้าในกลุ่ม HER ครบ 250 บาท(สุทธิ)
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}
			
			//*WR24032015
			if(promo_code=='OK27110315'){
				//รับส่วนลดพิเศษ 50 บาท เมื่อซื้อสินค้าในกลุ่ม B ครบ 500 บาท(สุทธิ)
				mstMcpPromotion(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}			
			
			if(promo_code=='OK27100115'){
				//รับส่วนลดพิเศษ 50 บาท เมื่อซื้อสินค้าในกลุ่ม HER ครบ 250 บาท(สุทธิ)
				lineApp(promo_code,chk_status,line_msg_error,idcard,coupon_code);
				return false;
			}
			
			//*WR08042015 
			if(promo_code=='OK27010415'){
				//รับส่วนลดพิเศษ 100 บาท เมื่อซื้อสินค้าในกลุ่ม B ครบ 1000 บาท(สุทธิ)
				$('#csh_product_id').focus();
				return false;
			}			
			
		}
		
	}//func
	 
	 function setPdt2Temp(promo_code){
			/***
			 * @desc : ซื้อ 1 แถม 1
			 * @create 20102014
			 * @modify 20102014
			 */
			$('<div id="dlgPdtMaster"><span style="color:#336666;font-size:20px;">รหัสสินค้า : </span><input type="text" size="15" id="pdt_master_id"/></div>').dialog({
			           autoOpen:true,
					   width:'400',
					   height:'200',	
					   modal:true,
					   resizable:false,
					   closeOnEscape:false,		
			           title: "สแกนรายการสินค้าหลักที่ร่วมโปรโมชั่น",
			           position:"center",
			           open:function(){
							$(this).dialog('widget')
					            .find('.ui-dialog-titlebar')
					            .removeClass('ui-corner-all')
					            .addClass('ui-corner-top');		
							//button style	
			  			    $(this).dialog("widget").find(".ui-btndlgpos")
			                  .css({"padding":"2","background-color":"#ffd1dc","font-size":"14px"});
			  			  $(this).dialog("widget").find(".ui-widget-overlay").live('click', function(){
						    	$("#pdt_redeempoint").focus();
							});
			  			  
			  			$('#pdt_master_id').focus();
			  			    
				  			  $('#pdt_master_id').keypress(function(evt){																    	
							    	var key = evt.keyCode ? evt.keyCode : evt.which ? evt.which : evt.charCode; 
				   			        if(key == 13){	
						    			evt.preventDefault();
						    			evt.stopPropagation();
						    			$('#btnMasterProduct').trigger('click');
						    			return false;
				   			        }
							    });
			           },
						buttons: [ 
					    	            { 
					    	                text: "ตกลง",
					    	                id:"btnMasterProduct",
					    	                class: 'ui-btndlgpos', 
					    	                click: function(evt){ 
					    	                	evt.preventDefault();
										 		evt.stopPropagation();
										 		var product_id=$('#pdt_master_id').val();
										 		product_id=$.trim(product_id);																		 			
									    		if(product_id.length<1){
								    				jAlert('กรุณาสแกนรหัสสินค้าหลักที่ร่วมรายการโปรโมชั่น', 'แจ้งเตือน',function(){
								    					$('#pdt_master_id').val('').focus();
								    					return false;
								    				});	
								    				return false;
									    		}
									    		//alert(m_promo_code + " ==> " + promo_id);
									    		var member_no=$('#csh_member_no').val();																													    	
									    		var status_no=$('#csh_status_no').val();
									    		var member_percent=$('#csh_percent_discount').html();//*WR17122014
					  							 $.ajax({
					  								 type:'post',
					  								 url:'/sales/member/setpd2temp',				  								 
					  								 data:{
					  									 member_no:member_no,																									  									
					  									 promo_code:promo_code,
					  									 product_id:product_id,
					  									 quantity:'1',
					  									 status_no:status_no,
					  									 seq_pro:'1',
					  									member_percent:member_percent,
					  									 rnd:Math.random()
					  								 },success:function(data){											  									
					  									var arr_data=data.split('#');
					  									if(arr_data[0]=='1'){	
					  										$('#dlgPdtMaster').dialog('close');	
					  										getCshTemp('P');
					  										////////////////////// show dialog product detail สินค้าตัวแถม /////////////////////
					  										var dlg_titledetail_show=$('#other_promotion_title').html();
					  										if($.trim(dlg_titledetail_show)==''){
					  											dlg_titledetail_show="สแกนรายการสินค้าฟรีที่ร่วมโปรโมชั่น";
					  										}
					  										$('<div id="dlgPdtDetail"><span style="color:#336666;font-size:20px;">รหัสสินค้า : </span><input type="text" size="15" id="pdt_detail_id"/></div>').dialog({
							  								           autoOpen:true,
							  										   width:'400',
							  										   height:'200',	
							  										   modal:true,
							  										   resizable:false,
							  										   closeOnEscape:false,		
							  										   title:dlg_titledetail_show,
							  								           position:"center",
							  								           open:function(){						  								        	
							  												$(this).dialog('widget')
							  										            .find('.ui-dialog-titlebar')
							  										            .removeClass('ui-corner-all')
							  										            .addClass('ui-corner-top');	
								  												//button style	
								  								  			    $(this).dialog("widget").find(".ui-btndlgpos")
								  								                  .css({"padding":"2","background-color":"#ffd1dc","font-size":"14px"});
								  								  			    
								  								  			   $(this).dialog("widget").find(".ui-widget-overlay").live('click', function(){
								  											    	$("#pdt_detail_id").focus();
								  												});
							  								  			  
							  								  			  	  $('#pdt_detail_id').focus();
							  								  			    
							  									  			  $('#pdt_detail_id').keypress(function(evt){																    	
							  												    	var key = evt.keyCode ? evt.keyCode : evt.which ? evt.which : evt.charCode; 
							  									   			        if(key == 13){	
							  											    			evt.preventDefault();
							  											    			evt.stopPropagation();
							  											    			$('#btnDetailProduct').trigger('click');
							  											    			return false;
							  									   			        }
							  												    });
							  								           },
							  											buttons: [ 
							  										    	            { 
							  										    	                text: "ตกลง",
							  										    	                id:"btnDetailProduct",
							  										    	                class: 'ui-btndlgpos', 
							  										    	                click: function(evt){ 
							  										    	                	evt.preventDefault();
							  															 		evt.stopPropagation();
							  															 		var product_id=$('#pdt_detail_id').val();
							  															 		product_id=$.trim(product_id);																		 			
							  														    		if(product_id.length<1){
							  													    				jAlert('กรุณาสแกนรหัสสินค้าฟรีที่ร่วมรายการโปรโมชั่น', 'แจ้งเตือน',function(){
							  													    					$('#pdt_detail_id').val('').focus();
							  													    					return false;
							  													    				});	
							  													    				return false;
							  														    		}
							  														    		//alert(m_promo_code + " ==> " + promo_id);
							  														    		var member_no=$('#csh_member_no').val();																													    	
							  														    		var status_no=$('#csh_status_no').val();
							  										  							 $.ajax({
							  										  								 type:'post',
							  										  								 url:'/sales/member/setpd2temp',
							  										  								 data:{
							  										  									 member_no:member_no,																									  									
							  										  									 promo_code:promo_code,
							  										  									 product_id:product_id,
							  										  									 quantity:'1',
							  										  									 status_no:status_no,
							  										  									seq_pro:'2',
							  										  									 rnd:Math.random()
							  										  								 },success:function(data){											  									
							  										  									var arr_data=data.split('#');
							  										  									if(arr_data[0]=='1'){
							  										  										$('#dlgPdtDetail').dialog('close');						  										  										
							  										  										//paymentBill('00');
							  										  										getCshTemp('P');	
							  										  										setPdt2Temp(promo_code);
							  										  										
							  														    				}else{													    					
							  														    					jAlert(arr_data[1],'ข้อความแจ้งเตือน',function(){
							  														    						$('#pdt_detail_id').val('').focus();
							  																					return false;
							  																				});
							  														    				}//end if									  									
							  										  									
							  										  								 }
							  										  							 });//ajax													 		
							  														 		
							  									    	                }
							  									    	            }
							  									    	  ]	
							  								           
							  								           
							  								         ,beforeClose:function(){
							  											//*WR28012015 =======================			    		  
							  				  							var start_baht=$('#csh_start_baht').val();
							  											var end_baht=$('#csh_end_baht').val();
							  											var buy_type=$("#csh_buy_type").val();//Gross,Net
							  											var buy_status=$("#csh_buy_status").val();//G >=,L<
							  											if(promo_code=='OK01160115'){
							  												//if(buy_type=='N' ){
							  													var net_amt=$('#csh_net').val().replace(/[^\d\.\-\ ]/g,'');
							  											 		net_amt=parseFloat(net_amt);
							  													var start_baht=start_baht.replace(/[^\d\.\-\ ]/g,'');
							  													 start_baht=parseFloat(start_baht);
							  													 var end_baht=end_baht.replace(/[^\d\.\-\ ]/g,'');
							  													 end_baht=parseFloat(end_baht);
							  													 //alert(net_amt +" > " +end_baht);
							  													 if(net_amt>end_baht){
							  														 jAlert("ยอดเงินสุทธิต้องไม่เกิน " + end_baht + " บาท กรุณาทำรายการใหม่", 'แจ้งเตือน',function(){																						
							  															$('#pdt_detail_id').val('').focus();
							  															return false;
							  														});
							  														 return false;																		    										 
							  													 }
							  												//}
							  											}//end if
							  											//*WR28012015 ===========================
							  										}
							  								           
							  								           
							  											,close:function(evt){
							  												if(evt.originalEvent && $(evt.originalEvent.target).closest(".ui-dialog-titlebar-close").length){	
							  													evt.stopPropagation();
							  													evt.preventDefault();	
							  													setTimeout(function(){
							  														paymentBill('00');
							  													},400);
								  												
							  												}else if(evt.originalEvent && $(evt.originalEvent.which==27)){
							  													evt.stopPropagation();
							  													evt.preventDefault();  						
							  												}
//							  												setTimeout(function(){
//							  													paymentBill('00');
//							  												},400);
							  												$('#dlgPdtDetail').remove();
							  								           }
							  								});
					  										////////////////////// show dialog product detail สินค้าตัวแถม /////////////////////
					  										
									    				}else{													    					
									    					jAlert(arr_data[1],'ข้อความแจ้งเตือน',function(){
									    						$('#pdt_master_id').val('').focus();
																return false;
															});
									    					
									    				}//end if									  									
					  									
					  								 }
					  							 });//ajax													 		
									 		
				    	                }
				    	            }
				    	  ]
						,close:function(evt){
							if(evt.originalEvent && $(evt.originalEvent.target).closest(".ui-dialog-titlebar-close").length){	
								evt.stopPropagation();
								evt.preventDefault();	
								setTimeout(function(){
										paymentBill('00');
									},400);
							}else if(evt.originalEvent && $(evt.originalEvent.which==27)){
								evt.stopPropagation();
								evt.preventDefault();  						
							}						
							$('#dlgPdtMaster').remove();
			           }
			});
		}//func
	
	function buyOneGetOne(promo_code){
		/***
		 * @desc : ซื้อ 1 แถม 1
		 * @create 20102014
		 * @modify 20102014
		 */
		$('<div id="dlgPdtMaster"><span style="color:#336666;font-size:20px;">รหัสสินค้า : </span><input type="text" size="15" id="pdt_master_id"/></div>').dialog({
		           autoOpen:true,
				   width:'400',
				   height:'200',	
				   modal:true,
				   resizable:false,
				   closeOnEscape:false,		
		           title: "สแกนรายการสินค้าหลักที่ร่วมโปรโมชั่น",
		           position:"center",
		           open:function(){
						$(this).dialog('widget')
				            .find('.ui-dialog-titlebar')
				            .removeClass('ui-corner-all')
				            .addClass('ui-corner-top');		
						//button style	
		  			    $(this).dialog("widget").find(".ui-btndlgpos")
		                  .css({"padding":"2","background-color":"#ffd1dc","font-size":"14px"});
		  			  $(this).dialog("widget").find(".ui-widget-overlay").live('click', function(){
					    	$("#pdt_redeempoint").focus();
						});
		  			  
		  			$('#pdt_master_id').focus();
		  			    
			  			  $('#pdt_master_id').keypress(function(evt){																    	
						    	var key = evt.keyCode ? evt.keyCode : evt.which ? evt.which : evt.charCode; 
			   			        if(key == 13){	
					    			evt.preventDefault();
					    			evt.stopPropagation();
					    			$('#btnMasterProduct').trigger('click');
					    			return false;
			   			        }
						    });
		           },
					buttons: [ 
				    	            { 
				    	                text: "ตกลง",
				    	                id:"btnMasterProduct",
				    	                class: 'ui-btndlgpos', 
				    	                click: function(evt){ 
				    	                	evt.preventDefault();
									 		evt.stopPropagation();
									 		var product_id=$('#pdt_master_id').val();
									 		product_id=$.trim(product_id);																		 			
								    		if(product_id.length<1){
							    				jAlert('กรุณาสแกนรหัสสินค้าหลักที่ร่วมรายการโปรโมชั่น', 'แจ้งเตือน',function(){
							    					$('#pdt_master_id').val('').focus();
							    					return false;
							    				});	
							    				return false;
								    		}
								    		//alert(m_promo_code + " ==> " + promo_id);
								    		var member_no=$('#csh_member_no').val();																													    	
								    		var status_no=$('#csh_status_no').val();
								    		var member_percent=$('#csh_percent_discount').html();//*WR17122014
				  							 $.ajax({
				  								 type:'post',
				  								 url:'/sales/member/setpdtbuyonegetone',				  								 
				  								 data:{
				  									 member_no:member_no,																									  									
				  									 promo_code:promo_code,
				  									 product_id:product_id,
				  									 quantity:'1',
				  									 status_no:status_no,
				  									 seq_pro:'1',
				  									 member_percent:member_percent,
				  									 rnd:Math.random()
				  								 },success:function(data){											  									
				  									var arr_data=data.split('#');
				  									if(arr_data[0]=='1'){	
				  										$('#dlgPdtMaster').dialog('close');	
				  										getCshTemp('P');
				  										////////////////////// show dialog product detail สินค้าตัวแถม /////////////////////
				  										var dlg_titledetail_show=$('#other_promotion_title').html();
				  										if($.trim(dlg_titledetail_show)==''){
				  											dlg_titledetail_show="สแกนรายการสินค้าฟรีที่ร่วมโปรโมชั่น";
				  										}
				  										$('<div id="dlgPdtDetail"><span style="color:#336666;font-size:20px;">รหัสสินค้า : </span><input type="text" size="15" id="pdt_detail_id"/></div>').dialog({
						  								           autoOpen:true,
						  										   width:'400',
						  										   height:'200',	
						  										   modal:true,
						  										   resizable:false,
						  										   closeOnEscape:false,		
						  										   title:dlg_titledetail_show,
						  								           position:"center",
						  								           open:function(){						  								        	
						  												$(this).dialog('widget')
						  										            .find('.ui-dialog-titlebar')
						  										            .removeClass('ui-corner-all')
						  										            .addClass('ui-corner-top');	
							  												//button style	
							  								  			    $(this).dialog("widget").find(".ui-btndlgpos")
							  								                  .css({"padding":"2","background-color":"#ffd1dc","font-size":"14px"});
							  								  			    
